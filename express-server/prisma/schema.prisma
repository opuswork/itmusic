model members {
  uno                    Int                      @id(map: "idx_36743_primary") @default(autoincrement())
  passwd                 String                   @db.VarChar(60)
  username               String                   @unique(map: "idx_36743_username") @db.VarChar(80)
  firstname              String                   @db.VarChar(30)
  lastname               String                   @db.VarChar(20)
  sex                    BigInt
  company_school         String                   @db.VarChar(100)
  position_status        String?                  @db.VarChar(50)
  study_area             String?                  @db.VarChar(50)
  school_name            String                   @db.VarChar(100)
  study_area2            String?                  @db.VarChar(255)
  study_area_memo        String?                  @db.VarChar(255)
  study_abroad           String?                  @db.VarChar(255)
  marketing              BigInt?
  major_teacher          String?                  @db.VarChar(128)
  webpage                String?                  @db.VarChar(255)
  contents               String?
  file_name1             String?                  @db.VarChar(128)
  file_name2             String?                  @db.VarChar(128)
  birthday               String?                  @db.VarChar(10)
  email                  String?                  @db.VarChar(255)
  phone                  String?                  @db.VarChar(20)
  mobile                 String?                  @default("") @db.VarChar(14)
  zipcode                String?                  @db.VarChar(7)
  addr1                  String?                  @db.VarChar(255)
  addr2                  String?                  @db.VarChar(128)
  state                  String?                  @db.VarChar(5)
  job                    String?                  @db.VarChar(128)
  signdate               DateTime?                @db.Timestamptz(6)
  userlevel              members_userlevel?       @default(LEVEL_1)
  membership_fee_paydate DateTime                 @db.Timestamptz(6)
  login_yb               members_login_yb?
  email_preference1      String?                  @default("0") @db.VarChar(10)
  email_preference2      String?                  @default("0") @db.VarChar(10)
  email_preference3      String?                  @default("0") @db.VarChar(10)
  terms_condition        members_terms_condition? @default(on)
  status                 members_status           @default(pending)

  @@schema("itmusic")
}

