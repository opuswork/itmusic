enum members_login_yb {
  Y
  N

  @@schema("itmusic")
}

enum members_status {
  approved
  pending

  @@schema("itmusic")
}

enum members_terms_condition {
  on
  off

  @@schema("itmusic")
}

enum members_userlevel {
  LEVEL_1  @map("1")
  LEVEL_12 @map("12")
  LEVEL_20 @map("20")
  LEVEL_80 @map("80")

  @@schema("itmusic")
}
